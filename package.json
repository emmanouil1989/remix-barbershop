    "@remix-validated-form/with-zod": "^2.0.7",
    "@types/bcrypt": "^5.0.2",
    "@types/nodemailer": "^6.4.14",
    "bcrypt": "^5.1.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
